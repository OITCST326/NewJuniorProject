@model IEnumerable<AIM.Web.Admin.Models.EntityModels.Application>

@{
    ViewBag.Title = "Index";
    var ActiveUserName = TempData["ActiveUserName"];
    TempData["ActiveUserName"] = ActiveUserName;
    var ActivePermissions = TempData["ActivePermissions"];
    TempData["ActivePermissions"] = ActivePermissions;
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicantId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JobId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsDays)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsEvenings)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsFullTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsWeekends)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SalaryExpectation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PreEmploymentStatement)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <th>
            @Html.DisplayFor(modelItem => item.ApplicationId)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.ApplicantId)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.JobId)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.IsDays)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.IsEvenings)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.IsFullTime)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.IsWeekends)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.SalaryExpectation)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.PreEmploymentStatement)
        </th>
        <th>
            @Html.DisplayFor(modelItem => item.Status)
        </th>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ApplicationId }) |
            @Html.ActionLink("Details", "Details", new { id = item.ApplicationId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.ApplicationId })
        </td>
    </tr>
}

</table>
